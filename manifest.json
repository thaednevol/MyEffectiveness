{
  "manifest_version": 2,
  "name": "MyEffectiveness 2",
  "version": "2",
  "description":"App to manage My Effectiveness",
  "permissions": ["tabs","activeTab", "https://ajax.googleapis.com/"],
  "background":
  {
	 "scripts": ["background.js","assets/jquery.js","assets/jquery-ui.min.js","js/my.js" ]
  },
  "browser_action":
  {
	"default_icon": "icon_128.png"
  },
  "content_scripts": [{
     "js": ["assets/jquery.js", "assets/jquery-ui.min.js","js/my.js"],
     "matches": ["http://*/*", "https://*/*"]
   }], 
  "minimum_chrome_version": "28"
}
